{
  "name": "@ocial/frontend",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://ocial.es/",
  "bugs": {
    "url": "https://github.com/isp-2324-ocial/frontend/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isp-2324-ocial/frontend/"
  },
  "author": "Ocial Team (https://github.com/isp-2324-ocial/frontend/graphs/contributors)",
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "scripts": {
    "analyze": "dpdm --no-warning --no-tree src/**/*.{ts,vue} && vite build --mode analyze",
    "build": "vite build",
    "check": "npm run lint && npm run typecheck && npm run test",
    "start": "vite",
    "serve": "vite preview",
    "lint": "eslint . --ignore-path .gitignore --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
    "lint-fix": "eslint . --fix --ignore-path .gitignore --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
    "prod": "npm run build && npm run serve",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "test": "npm run e2e-test && npm run unit-test",
    "e2e-test": "true",
    "unit-test": "true"
  },
  "dependencies": {
    "@vueuse/components": "10.9.0",
    "@vueuse/core": "10.9.0",
    "axios": "1.6.8",
    "blurhash": "2.0.5",
    "date-fns": "3.6.0",
    "defu": "6.1.4",
    "destr": "2.0.3",
    "fast-equals": "5.0.1",
    "inter-ui": "4.0.2",
    "leaflet": "1.9.4",
    "lodash-es": "4.17.21",
    "radix-vue": "1.5.3",
    "vue": "3.4.21",
    "vue-i18n": "9.10.2",
    "vue-router": "4.3.0",
    "vue3-google-login": "2.0.26",
    "vue3-toastify": "0.2.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@iconify/json": "2.2.196",
    "@intlify/eslint-plugin-vue-i18n": "3.0.0-next.7",
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@rollup/plugin-virtual": "3.0.2",
    "@stylistic/eslint-plugin": "1.7.0",
    "@types/geojson": "7946.0.14",
    "@types/leaflet": "1.9.8",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.12.2",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@vitejs/plugin-vue": "5.0.4",
    "browserslist": "4.23.0",
    "confusing-browser-globals": "1.0.11",
    "dpdm": "3.14.0",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-css": "0.9.2",
    "eslint-plugin-file-progress": "1.3.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.2",
    "eslint-plugin-jsonc": "2.15.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sonarjs": "0.25.0",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-vue": "9.24.0",
    "eslint-plugin-vue-scoped-css": "2.8.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.13.0",
    "lightningcss": "1.24.1",
    "rollup-plugin-visualizer": "5.12.0",
    "typescript": "5.4.3",
    "unocss": "0.58.9",
    "unplugin-icons": "0.18.5",
    "unplugin-vue-components": "0.26.0",
    "unplugin-vue-router": "0.8.5",
    "vite": "5.2.7",
    "vue-eslint-parser": "9.4.2",
    "vue-tsc": "2.0.7"
  },
  "engines": {
    "node": ">=20.8.1 <21.0.0",
    "npm": ">=10.1.0",
    "yarn": "Yarn is not supported. Please use NPM.",
    "pnpm": "PNPM is not supported. Please use NPM."
  }
}
